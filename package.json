  "buildEnvironment": {
    "development": {
      "mode": "development",
      "verbose": "false",
      "cssSourceMaps": "true",
      "cssAutoPrefixer": "true",
      "cssLinting": "true",
      "jsSourceMaps": "true",
      "jsLinting": "true",
      "notifications": "true"
    },
    "production": {
      "mode": "production",
      "verbose": "false",
      "cssSourceMaps": "false",
      "cssAutoPrefixer": "true",
      "cssLinting": "false",
      "jsSourceMaps": "false",
      "jsLinting": "false",
      "notifications": "false"
    }
  },
  "deployment": {
    "dataOptions": {
      "archivePath": "../data_impex",
      "lastDeploymentFileName": "lastbuild.properties",
      "uploadPath": "/on/demandware.servlet/webdav/Sites/Impex/src/instance/"
    },
    "dataBundles": {
      "core": [
        "core"
      ],
      "config": [
        "config_test"
      ],
      "data": [
        "data_test"
      ],
      "core-config": [
        "core",
        "config_test"
      ],
      "core-config-data": [
        "core",
        "data_test",
        "config_test"
      ],
      "core-config-data-static": [
        "core",
        "data_test",
        "config_test",
        "data_test_static"
